{
  "copyright": "Copyright (C) 2016 Charlie Poole",
  "authors": [ "Rob Prouse" ],
  "title": "NUnit .NET Core Runner Test Runner",
  "name": "dotnet-test-nunit.test.runner",
  "version": "3.4.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "NUnitLite": "3.5.0",
    "dotnet-test-nunit": {
      "target": "project"
    },
    "dotnet-test-nunit.test": {
      "target": "project"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dotnet5.6",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.IO.FileSystem": "4.0.1",
        "System.Console": "4.0.0"
      }
    },
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": {
          "version": "1.6.0",
          "type": "platform"
        },
        "System.IO.FileSystem": "4.0.1",
        "System.Console": "4.0.0"
      }
    },
    "net451": {}
  }
}
